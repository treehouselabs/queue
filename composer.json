{
  "name": "treehouselabs/queue",
  "type": "library",
  "description": "Queue",
  "keywords": ["mq","message queue","message","queue","amqp","rabbitmq","worker","pubsub"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl"
    }
  ],
  "require": {
    "php": ">=5.5",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "ext-amqp": "^1.7",
    "phpunit/phpunit": "^4.5",
    "doctrine/common": "^2.3",
    "doctrine/orm": "^2.3",
    "jms/serializer": "^0.14",
    "pdezwart/php-amqp": "^1.7@alpha"
  },
  "suggest": {
    "ext-amqp": "When you want to use the AMQP extension, written in C",
    "videlalvaro/php-amqplib": "When you want to use the PhpAmqpLib library, written in PHP"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\Queue\\": "src/TreeHouse/Queue/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\Queue\\Tests\\": "tests/TreeHouse/Queue/Tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
